#include<iostream>
rs
using namespace std;

int main()
{
	long long t,k,n,sum,i,temp,val;
	
	cin>>t;
	while(t)
	{	t--;
		cin>>n;
		cin>>k;
		sum=0;
		for(i=0;i<n;i++)
		{	cin>>temp;
			sum=sum+temp;	
		}
		
		if(sum%2==0)
		{
			if(k==1)
			cout<<"odd\n";
			else
			cout<<"even\n";
		}
		else
		{
			cout<<"even\n";
		}
		
	}
	return 0;
	
}
